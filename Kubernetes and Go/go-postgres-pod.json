{
  "kind": "Pod",
  "apiVersion": "v1",
  "metadata": {
    "name": "go-postgres",
    "labels": {
      "app": "go-postgres"
    }
  },
  "spec": {
    "containers": [
      {
        "name": "go",
        "image": "felixcao/gosql",
        "ports": [
          {
            "containerPort": 3000
          }
        ]
      },
	  
      {
        "name": "postdb",
        "image": "postgres",
        "ports": [
          {
            "containerPort": 5432,
            "protocol": "TCP"
          }
        ]
      }
    ]
  }
}